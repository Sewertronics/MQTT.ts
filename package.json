{
  "name": "mqtt.ts",
  "version": "1.0.0",
  "description": "This is an implementation of the MQTT 3.1.1 protocol for [Deno](https://deno.land/).",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdiamond/MQTT.ts.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jdiamond/MQTT.ts/issues"
  },
  "homepage": "https://github.com/jdiamond/MQTT.ts#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "serve": "^11.3.0",
    "terser": "^4.6.13"
  },
  "scripts": {
    "test": "npm run test:deno",
    "test:deno": "deno test client packets",
    "serve": "serve",
    "build": "npm run build:browser && npm run build:node",
    "build:browser": "npm run build:browser:esm && npm run build:browser:esm:min && npm run build:browser:umd && npm run build:browser:umd:min",
    "build:browser:esm": "deno bundle browser.ts > browser.mjs",
    "build:browser:esm:min": "terser browser.mjs -o browser.min.mjs",
    "build:browser:umd": "babel --config-file ./babel/browser.json browser.mjs > browser.js",
    "build:browser:umd:min": "terser browser.js -o browser.min.js",
    "build:node": "npm run build:node:esm && npm run build:node:cjs",
    "build:node:esm": "deno bundle node.ts > index.mjs",
    "build:node:cjs": "babel --config-file ./babel/node.json index.mjs > index.js"
  }
}
